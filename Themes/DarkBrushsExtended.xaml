<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"			
					xmlns:MLib_reskeys="clr-namespace:MLib.Themes;assembly=MLib">

    <SolidColorBrush x:Key="EditorSearchHeightBackgroundBrush" Color="#653306" />
    <Color x:Key="HighlightTextBlockBehavior">#006cbe</Color>
    <SolidColorBrush x:Key="IconBrush" Color="White" />
    <SolidColorBrush x:Key="IconInactiveBrush" Color="#66FFFFFF" />
    <SolidColorBrush x:Key="HeaderBackground" Color="#2D2D30" />
    <SolidColorBrush x:Key="StatusBarBackground" Color="#007ACC" />
    <SolidColorBrush x:Key="StatusBarForeground" Color="White" />
    <SolidColorBrush x:Key="SystemButtonsForeground" Color="White" />
    <SolidColorBrush x:Key="SystemButtonsInactiveForeground" Color="#818183" />
    <SolidColorBrush x:Key="SystemButtonsMouseOverBackground" Color="#393939" />
    <SolidColorBrush x:Key="SystemButtonsMousePressedForeground" Color="White" />
    <SolidColorBrush x:Key="SystemButtonsMousePressedBackground" Color="#007ACC" />
    <SolidColorBrush x:Key="WindowForeground" Color="White" />
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#007ACC" />
    <SolidColorBrush x:Key="WindowInactiveBorderBrush" Color="#434346" />
    <SolidColorBrush x:Key="ToolBarGripper" Color="#656565" />
    <SolidColorBrush x:Key="ProjectNameForeground" Color="White" />
    <SolidColorBrush x:Key="ProjectNameInactiveForeground" Color="#818181" />
    <SolidColorBrush x:Key="ProjectNameBackground" Color="#252526" />
    <SolidColorBrush x:Key="ScrollViewerRectBackground" Color="#333333" />
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type MLib_reskeys:ResourceKeys}, ResourceId=NormalBackgroundBrushKey}" Color="Transparent" />
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type MLib_reskeys:ResourceKeys}, ResourceId=TopLevelHeaderMenuBackgroundKey}" Color="Transparent" />
    <SolidColorBrush x:Key="EditorLineforegroundColor" Color="#C0C0C0" />
    <SolidColorBrush x:Key="EditorLineforegroundCurrColor" Color="#FFFFFF" />

    <BitmapImage x:Key="Image_OpenFile" UriSource="/LBC;component/Themes/Icons/Dark/OpenFile.ico" />

    <BitmapImage x:Key="Image_Copy" UriSource="/LBC;component/Themes/Icons/Dark/Copy.ico" />
    <BitmapImage x:Key="Image_Cut" UriSource="/LBC;component/Themes/Icons/Dark/Cut.ico" />
    <BitmapImage x:Key="Image_Paste" UriSource="/LBC;component/Themes/Icons/Dark/Paste.ico" />

    <BitmapImage x:Key="Image_Delete" UriSource="/LBC;component/Themes/Icons/Delete.ico" />

    <BitmapImage x:Key="Image_Undo" UriSource="/LBC;component/Themes/Icons/Dark/Undo.ico" />
    <BitmapImage x:Key="Image_Redo" UriSource="/LBC;component/Themes/Icons/Dark/Redo.ico" />

    <BitmapImage x:Key="HighlightingOnOff" UriSource="/LBC;component/Themes/Icons/Dark/desk-lamp.ico" />

    <!-- Word Wrap -->
    <BitmapImage x:Key="Image_WordWrap" UriSource="/LBC;component/Themes/Icons/Dark/WordWrap.png" />

    <!-- ShowLineNumbers appbar.list.png -->
    <BitmapImage x:Key="Image_ShowLineNumbers" UriSource="/LBC;component/Themes/Icons/Dark/ShowLineNumbers.png" />

    <!-- Show end of line character in text editor -->
    <BitmapImage x:Key="Image_EndOfLine" UriSource="/LBC;component/Themes/Icons/Dark/ShowEndOfLine.png" />

    <!-- Show Space character in text editor -->
    <BitmapImage x:Key="Image_Space" UriSource="/LBC;component/Themes/Icons/Dark/ShowSpaces.png" />

    <!-- Show Tab character in text editor -->
    <BitmapImage x:Key="Image_Tabs" UriSource="/LBC;component/Themes/Icons/Dark/TabStop.png" />

    <BitmapImage x:Key="Image_NewFile" UriSource="/LBC;component/Themes/Icons/Dark/NewFile.png" />

    <BitmapImage x:Key="Image_Openproject" UriSource="/LBC;component/Themes/Icons/Dark/Openproject.png" />

    <BitmapImage x:Key="Image_Save" UriSource="/LBC;component/Themes/Icons/Dark/Save.png" />

    <BitmapImage x:Key="Image_SaveAs" UriSource="/LBC;component/Themes/Icons/Dark/SaveAs.png" />

    <BitmapImage x:Key="Image_SaveAll" UriSource="/LBC;component/Themes/Icons/Dark/SaveAll.png" />

    <BitmapImage x:Key="Image_Comment" UriSource="/LBC;component/Themes/Icons/Dark/comment.png" />

    <BitmapImage x:Key="Image_Uncomment" UriSource="/LBC;component/Themes/Icons/Dark/uncomment.png" />

    <BitmapImage x:Key="CityImageFileText"    UriSource="/LBC;component/Themes/File/Dark/fileText.png" />
    <BitmapImage x:Key="CityImageIni"    UriSource="/LBC;component/Themes/File/Dark/ini.png" />
    <BitmapImage x:Key="CityImageRar"    UriSource="/LBC;component/Themes/File/Dark/rar.png" />
    <BitmapImage x:Key="CityImageZip"    UriSource="/LBC;component/Themes/File/Dark/zip.png" />
    <BitmapImage x:Key="CityImageHtml"    UriSource="/LBC;component/Themes/File/Dark/html.png" />
    <BitmapImage x:Key="CityImagePng"    UriSource="/LBC;component/Themes/File/Dark/png.png" />
    <BitmapImage x:Key="CityImageTxt"    UriSource="/LBC;component/Themes/File/Dark/txt.png" />
    <BitmapImage x:Key="CityImageDb"    UriSource="/LBC;component/Themes/File/Dark/db.png" />
    <BitmapImage x:Key="CityImageExcel"    UriSource="/LBC;component/Themes/File/Dark/Excel.png" />
    <BitmapImage x:Key="CityImageWord"    UriSource="/LBC;component/Themes/File/Dark/Word.png" />
    <BitmapImage x:Key="CityImagePdf"    UriSource="/LBC;component/Themes/File/Dark/Pdf.png" />
    <BitmapImage x:Key="CityImagePowerpoint"    UriSource="/LBC;component/Themes/File/Dark/Powerpoint.png" />
    <BitmapImage x:Key="Image_Back" UriSource="/LBC;component/Themes/Icons/Dark/back.png" />
    <BitmapImage x:Key="Image_Forward" UriSource="/LBC;component/Themes/Icons/Dark/forward.png" />
</ResourceDictionary>