<hc:Window  xmlns:hc="https://handyorg.github.io/handycontrol" x:Class="LBC.Tools.Autoload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LBC.Tools"
        mc:Ignorable="d"
        Title="自动加载配置" Height="450" Width="800"
         WindowState="Normal"
               WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize" Background="{DynamicResource HeaderBackground}"
        >
    <Window.Resources>
        <ContextMenu x:Key="CheckBoxContextMenu" Style="{StaticResource ContextMenu.Small}">
            
            <MenuItem Header="修改" Click="EditMenuItem_Click" Width="90" />
            
        </ContextMenu>
    </Window.Resources>
    <Grid>
        <TabControl TabStripPlacement="Top"  x:Name="iTabControl" Style="{StaticResource TabControlInLine}">
        </TabControl>
    </Grid>
</hc:Window>
